def Divisors(num):
  '''taking an input of an interger
  we return a string of numbers that can divide the input
  '''
  try:
    div=[]
    for i in range (1,num):
      if num%i==0:
        div.append(i)
    return str(div)[1:-1]
  except:
    return 'invalid input'



print(Divisors(int(input('input number '))))
